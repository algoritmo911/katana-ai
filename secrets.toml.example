# Example secrets configuration file for Katana AI
#
# Copy this file to secrets.toml and fill in your actual API keys and other secrets.
# IMPORTANT: secrets.toml is (and should be) ignored by Git (see .gitignore).
# Do NOT commit your actual secrets.

[coinbase]
api_key = "your-coinbase-api-key-here"
api_secret = "your-coinbase-api-secret-here"

# Add other sections for other services as needed
# [another_service]
# api_key = "your-other-service-key"

[supabase]
# URL of your Supabase project, found in Project Settings -> API
SUPABASE_URL = "your_supabase_url_here"
# Service role key (or anon key if appropriate for your RLS policies) for your Supabase project
# Found in Project Settings -> API. The service_role key bypasses RLS.
SUPABASE_KEY = "your_supabase_key_here"
